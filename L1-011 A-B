#include<stdio.h>
#include<string.h>
int main(){
    char a[100000],b[100000];
    gets(a);//这里gets函数的意思就是从输入缓冲区读取字符串或者字符指针
    gets(b);//char *gets(char *str);函数本身是这个样子

    for(int i=0;i<strlen(a);i++)
    {
        if(strchr(b,a[i]))//这里strchr函数是把a[i]和数组b进行比较，如果数组b中有ai就返回该数组位置，没有找到就返回NULL
        continue;        //所以我们可以用这个方法把重复的字母跳过，然后逐个输出字符
        else 
        printf("%c",a[i]);
    }
    return 0;
}


